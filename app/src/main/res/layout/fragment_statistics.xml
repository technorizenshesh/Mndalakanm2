<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.Home.Statistics.StatisticsFragment">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <LinearLayout
                    android:id="@+id/top"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_alignParentTop="true"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="@dimen/_10sdp"
                        android:drawableLeft="@drawable/time"
                        android:drawablePadding="@dimen/_15sdp"
                        android:drawableTint="@color/colorPrimary"
                        android:fontFamily="@font/poppins_bold"
                        android:gravity="start"
                        android:padding="@dimen/_5sdp"
                        android:text=" Accessibility"
                        android:textAlignment="textStart"
                        android:textColor="@color/black"
                        android:textSize="@dimen/_16sdp" />


                    <com.naqdi.chart.ChainChartView
                        android:id="@+id/chainChartView"
                        android:layout_width="match_parent"
                        android:layout_height="300dp"
                        android:layout_gravity="center"
                        android:layout_margin="@dimen/_5sdp" />

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_below="@id/top"
                    >
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="@dimen/_10sdp"
                        android:drawableStart="@drawable/time"
                        android:drawablePadding="@dimen/_15sdp"
                        android:drawableTint="@color/colorPrimary"
                        android:fontFamily="@font/poppins_bold"
                        android:gravity="start"
                        android:padding="@dimen/_5sdp"
                        android:text=" Telescope"
                        android:textAlignment="textStart"
                        android:textColor="@color/black"
                        android:textSize="@dimen/_16sdp" />
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        >
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/_2sdp"
                            android:drawableStart="@drawable/search_dr"
                            android:drawablePadding="@dimen/_15sdp"
                            android:drawableTint="@color/colorPrimary"
                            android:fontFamily="@font/poppins_medium"
                            android:gravity="start"
                            android:padding="@dimen/_5sdp"
                            android:text=" Web search"
                            android:textAlignment="textStart"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_14sdp" />
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/_5sdp"
                            android:fontFamily="@font/poppins_bold"
                            android:gravity="center"
                            android:padding="@dimen/_5sdp"
                            android:text=" No data yet"
                            android:textColor="@color/texts"
                            android:textSize="@dimen/_12sdp" />



                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        >
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/_2sdp"
                            android:drawableStart="@drawable/active_setting"
                            android:drawablePadding="@dimen/_15sdp"
                            android:fontFamily="@font/poppins_medium"
                            android:gravity="start"
                            android:padding="@dimen/_5sdp"
                            android:text="YouTube"
                            android:textAlignment="textStart"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_14sdp" />
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/_5sdp"
                            android:fontFamily="@font/poppins_bold"
                            android:gravity="center"
                            android:padding="@dimen/_5sdp"
                            android:text=" No data yet"
                            android:textColor="@color/texts"
                            android:textSize="@dimen/_12sdp" />



                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        >
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/_2sdp"
                            android:drawableStart="@drawable/time"
                            android:drawablePadding="@dimen/_15sdp"
                            android:drawableTint="@color/colorPrimary"
                            android:fontFamily="@font/poppins_medium"
                            android:gravity="start"
                            android:padding="@dimen/_5sdp"
                            android:text=" Web History"
                            android:textAlignment="textStart"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_14sdp" />
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/_5sdp"
                            android:fontFamily="@font/poppins_bold"
                            android:gravity="center"
                            android:padding="@dimen/_5sdp"
                            android:text=" No data yet"
                            android:textColor="@color/texts"
                            android:textSize="@dimen/_12sdp" />



                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        >
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/_2sdp"
                            android:drawableStart="@drawable/location"
                            android:drawablePadding="@dimen/_15sdp"
                            android:drawableTint="@color/colorPrimary"
                            android:fontFamily="@font/poppins_medium"
                            android:gravity="start"
                            android:padding="@dimen/_5sdp"
                            android:text=" Location"
                            android:textAlignment="textStart"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_14sdp" />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/_5sdp"
                            android:fontFamily="@font/poppins_bold"
                            android:gravity="center"
                            android:padding="@dimen/_5sdp"
                            android:text="0.0 km"
                            android:textColor="@color/texts"
                            android:textSize="@dimen/_16sdp" />

               <TextView
                   android:visibility="gone"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/_5sdp"
                            android:fontFamily="@font/poppins_bold"
                            android:gravity="center"
                            android:padding="@dimen/_5sdp"
                            android:text=" No data yet"
                            android:textColor="@color/texts"
                            android:textSize="@dimen/_12sdp" />
<ImageView
    android:layout_width="match_parent"
    android:layout_height="@dimen/_150sdp"
    android:src="@drawable/map_map"
    android:layout_margin="@dimen/_5sdp"
    android:scaleType="fitXY"/>


                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        >
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/_2sdp"
                            android:drawableStart="@drawable/time"
                            android:drawablePadding="@dimen/_15sdp"
                            android:drawableTint="@color/colorPrimary"
                            android:fontFamily="@font/poppins_medium"
                            android:gravity="start"
                            android:padding="@dimen/_5sdp"
                            android:text=" Schedules"
                            android:textAlignment="textStart"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_14sdp" />
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/_5sdp"
                            android:fontFamily="@font/poppins_bold"
                            android:gravity="center"
                            android:padding="@dimen/_5sdp"
                            android:text=" No data yet"
                            android:textColor="@color/texts"
                            android:textSize="@dimen/_12sdp" />



                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        >
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/_2sdp"
                            android:drawableStart="@drawable/time_rewards"
                            android:drawablePadding="@dimen/_15sdp"
                            android:drawableTint="@color/colorPrimary"
                            android:fontFamily="@font/poppins_medium"
                            android:gravity="start"
                            android:padding="@dimen/_5sdp"
                            android:text=" Time Reward's"
                            android:textAlignment="textStart"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_14sdp" />


                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/_5sdp"
                            android:fontFamily="@font/poppins_bold"
                            android:gravity="center"
                            android:padding="@dimen/_5sdp"
                            android:text="+2h 30m"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_16sdp" />

                        <androidx.cardview.widget.CardView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            app:cardElevation="@dimen/_3sdp"
                            app:cardUseCompatPadding="true"
                            app:cardCornerRadius="@dimen/_15sdp"
                            >
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                >
                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal"
                                    android:weightSum="2"
                                    >
                                    <TextView
                                        android:layout_weight="1"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_margin="@dimen/_2sdp"
                                        android:fontFamily="@font/poppins_medium"
                                        android:gravity="start"
                                        android:padding="@dimen/_5sdp"
                                        android:text=" Homework"
                                        android:textAlignment="textStart"
                                        android:textColor="@color/black"
                                        android:textSize="@dimen/_14sdp" />
                                    <TextView
                                        android:layout_weight="1"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_margin="@dimen/_2sdp"
                                        android:fontFamily="@font/poppinsregular"
                                        android:gravity="end"
                                        android:padding="@dimen/_5sdp"
                                        android:text=" + 30m"
                                        android:textAlignment="textEnd"
                                        android:textColor="@color/colorPrimary"
                                        android:textSize="@dimen/_10sdp" />
                                </LinearLayout>

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_margin="@dimen/_5sdp"
                                    android:fontFamily="@font/poppinsregular"
                                    android:gravity="center"
                                    android:padding="@dimen/_5sdp"
                                    android:text="Approved"
                                    android:textColor="@color/colorPrimary"
                                    android:textSize="@dimen/_12sdp" />
                            </LinearLayout>

                        </androidx.cardview.widget.CardView>
                        <androidx.cardview.widget.CardView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            app:cardElevation="@dimen/_3sdp"
                            app:cardUseCompatPadding="true"
                            app:cardCornerRadius="@dimen/_15sdp"
                            >
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                >
                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal"
                                    android:weightSum="2"
                                    >
                                    <TextView
                                        android:layout_weight="1"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_margin="@dimen/_2sdp"
                                        android:fontFamily="@font/poppins_medium"
                                        android:gravity="start"
                                        android:padding="@dimen/_5sdp"
                                        android:text=" Homework"
                                        android:textAlignment="textStart"
                                        android:textColor="@color/black"
                                        android:textSize="@dimen/_14sdp" />
                                    <TextView
                                        android:layout_weight="1"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_margin="@dimen/_2sdp"
                                        android:fontFamily="@font/poppinsregular"
                                        android:gravity="end"
                                        android:padding="@dimen/_5sdp"
                                        android:text=" + 30m"
                                        android:textAlignment="textEnd"
                                        android:textColor="@color/colorPrimary"
                                        android:textSize="@dimen/_10sdp" />
                                </LinearLayout>

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_margin="@dimen/_5sdp"
                                    android:fontFamily="@font/poppinsregular"
                                    android:gravity="center"
                                    android:padding="@dimen/_5sdp"
                                    android:text="Approved"
                                    android:textColor="@color/colorPrimary"
                                    android:textSize="@dimen/_12sdp" />
                            </LinearLayout>

                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            app:cardElevation="@dimen/_3sdp"
                            app:cardUseCompatPadding="true"
                            app:cardCornerRadius="@dimen/_15sdp"
                            >
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                >
                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal"
                                    android:weightSum="2"
                                    >
                                    <TextView
                                        android:layout_weight="1"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_margin="@dimen/_2sdp"
                                        android:fontFamily="@font/poppins_medium"
                                        android:gravity="start"
                                        android:padding="@dimen/_5sdp"
                                        android:text=" Homework"
                                        android:textAlignment="textStart"
                                        android:textColor="@color/black"
                                        android:textSize="@dimen/_14sdp" />
                                    <TextView
                                        android:layout_weight="1"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_margin="@dimen/_2sdp"
                                        android:fontFamily="@font/poppinsregular"
                                        android:gravity="end"
                                        android:padding="@dimen/_5sdp"
                                        android:text=" + 30m"
                                        android:textAlignment="textEnd"
                                        android:textColor="@color/colorPrimary"
                                        android:textSize="@dimen/_10sdp" />
                                </LinearLayout>

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_margin="@dimen/_5sdp"
                                    android:fontFamily="@font/poppinsregular"
                                    android:gravity="center"
                                    android:padding="@dimen/_5sdp"
                                    android:text="Approved"
                                    android:textColor="@color/colorPrimary"
                                    android:textSize="@dimen/_12sdp" />
                            </LinearLayout>

                        </androidx.cardview.widget.CardView>

                    </LinearLayout>

                </LinearLayout>


            </RelativeLayout>


        </LinearLayout>


    </androidx.core.widget.NestedScrollView>

</layout>
